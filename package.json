{
	"name": "eg-notes",
	"description": "EG Colors",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "notes-panel",
					"title": "Notes",
					"icon": "$(heart)"
				}
			]
		},
		"views": {
			"notes-panel": [
				{
					"type": "webview",
					"id": "calicoColors.colorsView",
					"name": "Notes View"
				},
				{
					"id": "notesExplorer",
					"name": "Notes Explorer",
					"icon": "media/dep.svg",
					"contextualTitle": "Notes Explorer",
					"contents": "No node dependencies found [learn more](https://www.npmjs.com/).\n[Add Dependency](command:nodeDependencies.addEntry)"
				}
			]
		},
		"commands": [
			{
				"command": "calicoColors.addNote",
				"category": "Notes",
				"title": "Add Note"
			},
			{
				"command": "calicoColors.clearColors",
				"category": "Notes",
				"title": "Clear All",
				"icon": "$(clear-all)"
			},
			{
        "command": "notesExplorer.refreshEntry",
        "title": "Refresh my view"
      }
		],
		"menus": {
			"view/title": [
				{
					"command": "calicoColors.clearColors",
					"group": "navigation",
					"when": "view == calicoColors.colorsView"
				},
				{
					"command": "notesExplorer.refreshEntry",
					"when": "view == notesExplorer",
					"icon": "$(heart)"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/node": "^20.8.9",
		"@types/vscode": "^1.73.0",
		"@types/vscode-webview": "^1.57.3",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.32.0",
		"typescript": "^5.2.2"
	}
}